config BR2_PACKAGE_KUBOS_COMMAND_AND_CONTROL
	bool "command_and_control"
	default y
	help
		Include KubOS Linux Command and Control service.
		This service allows for running commands
		through the command and control daemon.

config BR2_KUBOS_COMMAND_AND_CONTROL_INIT_LVL
	int "Command and Control Run Level"
	default 70
	range 10 99
	depends on BR2_PACKAGE_KUBOS_COMMAND_AND_CONTROL
	help
		The initialization priority level of the KubOS Linux
		command and control service.  The lower the number,
		the earlierthe service is initialized.
